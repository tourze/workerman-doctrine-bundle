{
  "name": "tourze/workerman-doctrine-bundle",
  "description": "Workerman + DoctrineORM",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-pcntl": "*",
    "symfony/event-dispatcher": "^6.4",
    "doctrine/orm": "^2.20 || ^3.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "workerman/workerman": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\WorkermanDoctrineBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\WorkermanDoctrineBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "maglnet/composer-require-checker": "^4"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}